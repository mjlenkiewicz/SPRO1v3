h1 {
	margin-top: 10px;
	margin-bottom: 10px;
	font-size: 3vw;
}

h2 {
	margin-top: 10px;
	margin-bottom: 10px;
	font-size: 2vw;
}

h3 {
	margin-top: 10px;
	margin-bottom: 10px;
	font-size: 1.5vw;
}


p {
	font-size: 2.5vw;
	line-height: 3vw;
	text-align: justify;
	text-shadow: 1px 5px 3px grey;
	margin-left: auto;
    margin-right: auto
}

ul {
	color:white;
	font-size: 2vw;
	line-height: 3vw;
	text-align: justify;
	text-shadow: 1px 1px 2px grey;
	margin-left: auto;
    margin-right: auto;
}

.highlight {			
	  position: relative;		
	  display: inline-block;		
	}		
			
.highlight:before {			
	  content: " ";		
	  display: block;		
	  height: 55%;		
	  width: 100%;		
	  margin-left: -3px;		
	  margin-right: -3px;		
	  position: absolute;		
	  background: linear-gradient(to right bottom, #5eff6b, #55ed6d, #4edc6e, #49ca6e, #46b96c);		
	  transform: rotate(-1.5deg) translate(0%, 35%) scale(95%);		
	  top: -1px;		
	  left: -1px;		
	  border-radius: 5% 2% 5% 5%;		
	  padding: 5px 3px 3px 5px;		
	  z-index:-1;		
	}		

/* Fade In Effect */

.fade-in {
  	opacity: 0;
  	filter: blur(3px);
	transform: scale(0.97);
	}

.fade-in:nth-child(1) {
	animation: fade-IN 0.9s 0.1s forwards cubic-bezier(0.15, 0, 0.5, 0);
	}

.fade-in:nth-child(2) {
	animation: fade-IN 0.9s 0.2s forwards cubic-bezier(0.15, 0, 0.5, 0);
	}
	
.fade-in:nth-child(3) {
	animation: fade-IN 0.9s 0.3s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(4) {
	animation: fade-IN 0.9s 0.4s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(5) {
	animation: fade-IN 0.9s 0.5s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(6) {
	animation: fade-IN 0.9s 0.6s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(7) {
	animation: fade-IN 0.9s 0.7s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(8) {
	animation: fade-IN 0.9s 0.8s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

.fade-in:nth-child(9) {
	animation: fade-IN 0.9s 0.9s forwards cubic-bezier(0.15, 0, 0.5, 0);
}

@keyframes fade-IN {
	
	80% {
		opacity: 0.8;
		filter: blur(0);
		transform: scale(1.1);
		}
	100% {
		opacity: 1;
		filter: blur(0);
		transform: scale(1);
		}
	}

