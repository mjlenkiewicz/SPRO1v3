.glitch {
    position: relative;
    font-weight: 700;
    letter-spacing: 0.25rem;
    text-shadow: 0.05em 0 0 rgb(255 0 0 / 75%), -0.025em -0.05em 0 rgb(0 255 0 / 75%), 0.025em 0.05em 0 rgb(0 0 255 / 75%);
    animation: glitch 500ms infinite;
	}

@keyframes glitch{
	0%{
		text-shadow:
		0.05em 0 0 rgba(255, 0, 0, .75),
		-0.025em -.05em 0 rgba(0, 255, 0, .75),
		0.025em .05em 0 rgba(0, 0, 255, .75);
		}
	14%{
		text-shadow:
		0.05em 0 0 rgba(255, 0, 0, .75),
		-0.025em -.05em 0 rgba(0, 255, 0, .75),
		0.025em .05em 0 rgba(0, 0, 255, .75);
		}
	15%{
		text-shadow:
		-0.05em -.025em 0 rgba(255, 0, 0, .75),
		0.025em 0.025em 0 rgba(0, 255, 0, .75),
		-0.05em -.05em 0 rgba(0, 0, 255, .75);
		}
	49%{
		text-shadow:
		-0.05em -.025em 0 rgba(255, 0, 0, .75),
		0.025em 0.025em 0 rgba(0, 255, 0, .75),
		-0.05em -.05em 0 rgba(0, 0, 255, .75);
		}
	50%{
		text-shadow:
		0.025em 0.05em 0 rgba(255, 0, 0, .75),
		0.05em 0 0 rgba(0, 255, 0, .75),
		0 -0.05em 0 rgba(0, 0, 255, .75);
		}
	99%{
		text-shadow:
		0.025em 0.05em 0 rgba(255, 0, 0, .75),
		0.05em 0 0 rgba(0, 255, 0, .75),
		0 -0.05em 0 rgba(0, 0, 255, .75);
		}
	100%{
		text-shadow:
		-0.025em 0 0 rgba(255, 0, 0, .75),
		-0.025em -0.025em 0 rgba(0, 255, 0, .75),
		-0.025em -0.05em 0 rgba(0, 0, 255, .75);
		}
	}

	@keyframes move {
		100% {
			transform: translate3d(0, 0, 1px) rotate(360deg);
		}
	}

/*** RESPONSIVE ***/
/***** MOBILE *****/
 
@media only screen and (orientation: portrait) {
	body {			
		flex-direction: column;
	}

	section {
		font-size: 5vw;
		line-height: 5vw;
		text-align: center;
	}

	h1 {
		font-size: 5.5vw;
		line-height: 7vw; 	
		text-align: center; 	
		} 	
		
	h2 {	
		font-size: 3.5vw;	
		} 	
	
	h3 {
		font-size: 2.5vw;
		}

	p {	
		font-size: 7vw;
		line-height: 10vw;
		text-align: center;
		}
		
	ul {
		font-size: 4vw;
		line-height: 6.5vw;	
		margin-right:11vw;
		margin-left:3.5vw;
		}

	.button {
		transform:scale(2.25);
		margin: 4.5vw;
		}

	.button:hover, .button:focus {
		transform:scale(2.25);
		} 		
			
			
}

@media only screen and (orientation: landscape) {
			
	body {		
		flex-direction: row;
	}

	ul {
		margin-right:7.5vw;
		margin-left:2vw;
		}

	.button {
		transform:scale(1.25);
		margin: 1.5vw;
		}

	.button:hover, .button:focus {
		transform:scale(1.25);
		} 	

}


