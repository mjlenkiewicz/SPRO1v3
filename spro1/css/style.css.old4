body::-webkit-scrollbar {
    display: none;
}

section {
	display: flex;
	flex-wrap: wrap;
    flex-direction: column;
	min-height: 100vh;	
	height: 100vh;
	padding: 5%;
    align-items: center;
	align-content: center;
    justify-content: center;
	margin: 0;
	border: 0;
	top: 50%;
	left: 50%;
    color: #FFFFFF;
	font-size: 2.5vw;
    text-align: center;
	font-weight: lighter;
	line-height: 150%;
    opacity: 1;
	/*** Scroll Snap ***/
	scroll-snap-align: center;
	}

