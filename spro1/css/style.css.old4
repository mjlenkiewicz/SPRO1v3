/** BUTTONS & EFFECTS **/

.button {
	width: 20vw;
	height: 2.75vw;
	margin: 2vw;
	padding: -2vw 7vw;
	border: none;
/*	border: 3px solid black; */
	border-radius: 5rem 10rem;
	background: linear-gradient(45deg, #4528bf 0%, #00ccc9 100%);
	color: white;
	font-family: Rubik;
	font-size: 1.3vw;
	font-weight: bold;
	transition: background-color 0.2s ease, color 0.2s ease;
	transform: translate(0,0);
	transition: 300ms;
	cursor: pointer;
	margin-left: auto;
    margin-right: auto
}

.button:hover, .button:focus {
	background: transparent;
	padding: -2vw 7vw;
	border: none;
	color: black;
	transform: translate(0.5vw,0.5vw);
	transition: 500ms;
}

