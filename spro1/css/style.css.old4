html {
  scroll-behavior: smooth;
  scroll-margin-block-end: 0;
  scroll-snap-type: y mandatory;
  -ms-overflow-style: none;
  overflow: hidden scroll;
}

body {
    margin: 0;
    padding: 0;
	font-family: Rubik;
